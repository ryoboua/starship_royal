{
  "name": "starshiproyal",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "backend": "nodemon ./backend/server.js",
